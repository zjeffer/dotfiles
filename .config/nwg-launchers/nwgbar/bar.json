[
  {
    "name": "Lock screen",
    "exec": "$HOME/.config/hypr/apps/swaylock/lock.sh",
    "icon": "system-lock-screen"
  },
  {
    "name": "Logout",
    "exec": "hyprctl dispatch exit",
    "icon": "system-log-out"
  },
  {
    "name": "Reboot",
    "exec": "systemctl reboot",
    "icon": "system-reboot"
  },
  {
    "name": "Shutdown",
    "exec": "systemctl -i poweroff",
    "icon": "system-shutdown"
  }
]
