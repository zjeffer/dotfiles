[module/nm-launcher]
type = custom/script
exec = ~/.config/polybar/scripts/nm-applet/applet.sh
label = %output% 
interval = 2
click-left = nm-connection-editor &
click-middle = ~/.config/polybar/scripts/nm-applet/applet.sh --ssid &


label-padding-left = 1
format-prefix = %{T7}%{T-}
format-prefix-foreground = ${colors.wifi}
format-prefix-background = ${colors.temp}
format-suffix = %{T7}%{T-}
format-suffix-foreground = ${colors.wifi}
format-suffix-background = ${colors.wifi}
format-background = ${colors.wifi}
